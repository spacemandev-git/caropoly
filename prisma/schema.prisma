// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Players {
  wallet String @id
  rent   Int
}

model Targets {
  id            String @id @default(uuid())
  owner         String
  programId     String
  instruction   String
  targetType    String
  targetAddress String
}

model PendingTargets {
  id          String @id @default(uuid())
  owner       String
  programId   String
  instruction String
  targetType  String
}
